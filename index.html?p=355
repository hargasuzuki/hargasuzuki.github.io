	 	<script>
		  // Scroll Top
			$(window).scroll(function() {
				if ($(this).scrollTop() >= 150) {
					$('#return-to-top').fadeIn(200);
					$('body').addClass("fade");
			 	} else {
					$('#return-to-top').fadeOut(200);
					$('.open').css("display","block");
					$('body').removeClass("fade");
			 	}
			});
			$('#return-to-top').click(function() {
				$('body,html').animate({
					scrollTop : 0
			 	}, 500);
			});
			
		</script>
		

